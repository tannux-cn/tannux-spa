<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:component-scan base-package="com.tannux.demo.spa.server.**" />
	
	
	<!-- peer -->
	<bean class="com.tannux.demo.spa.server.peer.impl.SysMenuPeerImpl" />
	<bean class="com.tannux.demo.spa.server.peer.impl.SysRolePeerImpl" />
	<bean class="com.tannux.demo.spa.server.peer.impl.SysUserPeerImpl" />
	
	
	
	<!-- api -->
	<bean class="com.tannux.demo.spa.server.api.impl.SysMenuApiImpl" />
	<bean class="com.tannux.demo.spa.server.api.impl.SysRoleApiImpl" />
	<bean class="com.tannux.demo.spa.server.api.impl.SysUserApiImpl" />
	<bean class="com.tannux.demo.spa.server.api.impl.AuthApiImpl" />
	
	
	
</beans>



